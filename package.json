    "@sa/materials": "workspace:*",
    "@sa/utils": "workspace:*",
    "ahooks": "3.9.3",
    "antd": "5.27.0",
    "clsx": "2.1.1",
    "dayjs": "1.11.13",
    "echarts": "6.0.0",
    "i18next": "25.3.6",
