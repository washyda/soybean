    "vite-plugin-svg-icons": "2.0.1"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm sa git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "website": "https://react.soybeanjs.cn/home/"
