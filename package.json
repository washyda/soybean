    "@unocss/preset-icons": "66.4.2",
    "@unocss/preset-uno": "66.4.2",
    "@unocss/transformer-directives": "66.4.2",
    "@unocss/transformer-variant-group": "66.4.2",
    "@unocss/vite": "66.4.2",
    "boxen": "8.0.1",
